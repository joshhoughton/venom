{
  "name": "Venom - Spotify + YouTube Bookmarker",
  "version": "0.2.3",
  "manifest_version": 2,
  "description": "Instantly save the timestamp of playing Spotify, YouTube or SoundCloud media, on screen or in the background.",
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net https://ajax.googleapis.com; object-src 'self';",
  "key": "",
  "icons": {
    "128": "icons/icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
        "lib/spotify-web-api.js",
        "lib/moment.js",
        "src/utils/utils.js",
        "src/platforms/platforms.js",
        "src/background/background.js"
    ]
  },
  "options_page": "src/options/options.html",
  "browser_action": {
      "default_icon": "icons/icon.png",
      "title": "See your Bookmarks"
  },
  "permissions": [
    "*://*.youtube.com/watch?v=*",
    "*://*.soundcloud.com/*",
    "https://accounts.spotify.com/authorize/*",
    "tabs",
    "downloads",
    "activeTab",
    "storage",
    "notifications",
    "identity"
  ],
  "commands": {
    "record-timestamp": {
      "suggested_key": {
        "default": "Alt+Q",
        "mac": "Alt+Q"
      },
      "description": "Bookmark playing media"
    }
  }
}